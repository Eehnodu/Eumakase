<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.db.MusicMapper">
   <resultMap id="musicResultMap" type="com.smhrd.model.MusicVO">
        <result property="title" column="title"/>
        <result property="artist" column="artist"/>
   </resultMap>

   <select id="getMusic" parameterType="com.smhrd.model.AiPlaylistVO" resultMap="musicResultMap">
        SELECT title, artist
        FROM tb_music
        WHERE musicIdx = #{musicIdx}
   </select>
</mapper>
